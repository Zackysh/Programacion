package arrays;

public class MaxMin {
	public static void main(String[] args) {
		
		int[] a = new int[4];
		int indiceMin = 0;
		int indiceMax = 0;
		
		for (int i = 0; i < a.length; i++) {
			a[i] = (int) (Math.random() * 11);
		}
		
		indiceMax = Integer.MAX_VALUE;
		
		System.out.println(indiceMax);
		
	}
}
